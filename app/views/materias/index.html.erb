<h1>Listing materias</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Descripcion</th>
  </tr>

<% @materias.each do |materia| %>
  <tr>
    <td><%=h materia.titulo %></td>
    <td><%=h materia.descripcion %></td>
    <td><%= link_to 'Show', materia %></td>
    <td><%= link_to 'Edit', edit_materia_path(materia) %></td>
    <td><%= link_to 'Destroy', materia, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New materia', new_materia_path %>